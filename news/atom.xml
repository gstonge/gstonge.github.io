---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>News</title>
	<link href="http://gstonge.github.io/news/atom.xml" rel="self"/>
	<link href="http://gstonge.github.io/news"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://gstonge.github.io/news</id>
	<author>
		<name>Guillaume St-Onge</name>
		<email>guillaume.st-onge.4@ulaval.ca</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://gstonge.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://gstonge.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
